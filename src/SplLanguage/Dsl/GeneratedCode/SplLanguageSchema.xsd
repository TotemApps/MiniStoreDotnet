    <xsd:annotation>
      <xsd:documentation>Moniker for FeatureModelHasModelConstraints instance.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="core:ModelElementMoniker">
        <xsd:attribute name="Id" type="xsd:string" >
